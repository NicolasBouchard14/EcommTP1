<div class="col-sm-6">
  <div class="card h-100">
    <h3 class="card-header">Étude #<%= index %></h3>
    <div class="card-body">
      <div class="form-group string optional disabled etude_secteurEtudes">
        <label class="control-label string optional disabled" for="etude_secteurEtudes">Secteur</label>
        <input id="etude_secteurEtudes" class="form-control string optional disabled" disabled="disabled" value="<%= etude.secteurEtudes %>" type="text">
      </div>
      <div class="form-group string optional disabled etude_niveau">
        <label class="control-label string optional disabled" for="etude_niveau">Niveau</label>
        <input id="etude_niveau" class="form-control string optional disabled" disabled="disabled" value="<%= etude.niveau %>" type="text">
      </div>
      <div class="form-group string optional disabled etude_dateDebut">
        <label class="control-label string optional disabled" for="etude_dateDebut">Date de début</label>
        <input id="etude_dateDebut" class="form-control string optional disabled" disabled="disabled" value="<%= etude.dateDebut.to_date %>" type="text">
      </div>
      <div class="form-group string optional disabled etude_dateCompletion">
        <label class="control-label string optional disabled" for="etude_dateCompletion">Date de complétion</label>
        <input id="etude_dateCompletion" class="form-control string optional disabled" disabled="disabled" value="<%= etude.dateCompletion.to_date %>" type="text">
      </div>
      <div class="card">
        <h5 class="card-header">Institution</h5>
        <div class="form-group string optional disabled institution_nom" style="padding: 0px 10px 0px 10px">
          <label class="control-label string optional disabled" for="institution_nom">Nom</label>
          <input id="institution_nom" class="form-control string optional disabled" disabled="disabled" value="<%= etude.institution.nom %>" type="text">
        </div>
        <div class="form-group string optional disabled institution_numero_civique" style="padding: 0px 10px 0px 10px">
          <label class="control-label string optional disabled" for="institution_numero_civique">Numéro civique</label>
          <input id="institution_numero_civique" class="form-control string optional disabled" disabled="disabled" value="<%= etude.institution.addresse.numeroCivique %>" type="text">
        </div>
        <div class="form-group string optional disabled institution_rue" style="padding: 0px 10px 0px 10px">
          <label class="control-label string optional disabled" for="institution_rue">Rue</label>
          <input id="institution_rue" class="form-control string optional disabled" disabled="disabled" value="<%= etude.institution.addresse.rue %>" type="text">
        </div>
        <div class="form-group string optional disabled institution_codePostal" style="padding: 0px 10px 0px 10px">
          <label class="control-label string optional disabled" for="institution_codePostal">Code Postal</label>
          <input id="institution_codePostal" class="form-control string optional disabled" disabled="disabled" value="<%= etude.institution.addresse.codePostal %>" type="text">
        </div>
        <div class="form-group string optional disabled institution_ville" style="padding: 0px 10px 0px 10px">
          <label class="control-label string optional disabled" for="institution_ville">Ville</label>
          <input id="institution_ville" class="form-control string optional disabled" disabled="disabled" value="<%= etude.institution.addresse.ville %>" type="text">
        </div>
        <div class="form-group string optional disabled institution_province" style="padding: 0px 10px 0px 10px">
          <label class="control-label string optional disabled" for="institution_province">Province</label>
          <input id="institution_province" class="form-control string optional disabled" disabled="disabled" value="<%= etude.institution.addresse.province %>" type="text">
        </div>
      </div>
    </div>
  </div>
</div>
